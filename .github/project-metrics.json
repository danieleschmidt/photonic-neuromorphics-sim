  "alerts": {
    "critical": [
      {
        "metric": "uptime",
        "condition": "< 99.0",
        "notification": ["email", "slack", "pagerduty"]
      },
      {
        "metric": "vulnerabilities.critical",
        "condition": "> 0",
        "notification": ["email", "slack"]
      },
      {
        "metric": "secrets_exposed",
        "condition": "> 0",
        "notification": ["email", "slack", "pagerduty"]
      }
    ],
    "warning": [
      {
        "metric": "test_coverage",
        "condition": "< 80",
        "notification": ["slack"]
      },
      {
        "metric": "build_time",
        "condition": "> 600",
        "notification": ["slack"]
      },
      {
        "metric": "error_rate",
        "condition": "> 1.0",
        "notification": ["email", "slack"]
      }
    ]
  },
  "automation": {
    "data_collection": {
      "frequency": "daily",
      "script": "scripts/collect_metrics.py",
      "storage": "prometheus,influxdb"
    },
    "report_generation": {
      "frequency": "weekly",
      "script": "scripts/generate_reports.py",
      "distribution": ["email", "slack"]
    },
    "trend_analysis": {
      "frequency": "monthly",
      "script": "scripts/analyze_trends.py",
      "output": "trend_analysis_report.pdf"
    }
  },
